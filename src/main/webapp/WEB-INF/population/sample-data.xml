		<property name="description" value="Lorem ipsum sit dolor amet."></property>
		<property name="moreInfo" value="https://www.youtube.com/watch?v=Bv1jALmLfQQ"></property>
		<property name="finalMode" value="true"></property>
		<property name="employer" value="employer1Employer"></property>
