		<property name="text" value="Im spanish!"/>
		<property name="info" value="https://www.youtube.com/watch?v=GWCldYPEsl4"/>
	</bean>
	
		
	<bean name="employer1UserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="employer1"></property>
		<property name="password" value="employer1"></property>
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="Employer"></property>
				<property name="surname" value="One"></property>
				<property name="email" value="employer1@gmail.com"></property>
				<property name="phone" value="+44 123-456-789"></property>
			</bean>
		</property>
		<property name="enabled" value="true"></property>
		<property name="roles">
			<list>
				<ref bean="employer1Employer"/>
				<ref bean="employer1Authenticated"/>
			</list>
		</property>
	</bean>
	
	<bean name="employer1Employer" class="acme.entities.roles.Employer">
		<property name="company" value="Employer 1, Inc."></property>
		<property name="sector" value="Sector 1"></property>
		<property name="userAccount" ref="employer1UserAccount"></property>
	</bean>
	
	<bean name="employer1Authenticated" class="acme.framework.entities.Authenticated">
		<property name="userAccount" ref="employer1UserAccount"></property>
	</bean>
	
	
	<bean name="employer2UserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="employer2"></property>
		<property name="password" value="employer2"></property>
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="Employer2"></property>
				<property name="surname" value="Two"></property>
				<property name="email" value="employer2@gmail.com"></property>
				<property name="phone" value="+44 123-456-781"></property>
			</bean>
		</property>
		<property name="enabled" value="true"></property>
		<property name="roles">
			<list>
				<ref bean="employer2Employer"/>
				<ref bean="employer2Authenticated"/>
			</list>
		</property>
	</bean>
	
	<bean name="employer2Employer" class="acme.entities.roles.Employer">
		<property name="company" value="Employer 2, Inc."></property>
		<property name="sector" value="Sector 2"></property>
		<property name="userAccount" ref="employer2UserAccount"></property>
	</bean>
	
	<bean name="employer2Authenticated" class="acme.framework.entities.Authenticated">
		<property name="userAccount" ref="employer2UserAccount"></property>
	</bean>
	
	<bean name="job1" class="acme.entities.jobs.Job">
